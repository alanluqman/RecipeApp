<%= render 'partials/navigation'%>
<div class = 'container'>
  <% @recipes.each do |recipe| %>
    <div class = 'recipe_card'>
      <a href = "/users/<%= @user.id %>/recipes/<%= recipe.id %> ">
        <h1><%= recipe.name %></h1>
        <p><%= recipe.description %></p>
      </a>
    </div>
  <% end %>
</div>